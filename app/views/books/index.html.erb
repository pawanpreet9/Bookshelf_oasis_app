
<h1>All Books</h1>
<%= paginate @books %>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Quantity</th>

    </tr>
  </thead>
  <tbody>
    <% @books.each do |book| %>
      <tr>
        <td><%= link_to book.title, book_path(book) %></td>

        <td><%= book.quantity %></td>

      </tr>
    <% end %>
  </tbody>
</table>
